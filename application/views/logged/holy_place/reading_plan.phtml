				data: '',
				success: function (msg) {
						$('#catdiv').html(msg.html);
						
				}	// end of success function...
			});
	});
	
	$('#editCatBtn').click(function(){
	$.ajax({
			type: 'post',
			url: '<?php echo base_url()?>logged/holy_place/edit_category/',
			dataType: 'json',
			data: $('#frmeditcat').serialize(),
			success: function (msg) {
					if(msg.success==false)
					{
						if(msg.data.err_addcat)
						{
							$('#err_editcat').html(msg.data.err_addcat);
							
						}
					}
					else
					{
						$('#catdiv').html(msg.html);
						hide_dialog();
						showUIMsg(msg.data);
					}
			}
	});
});
	
});
function submitPlan(plan_type)
{
	$.ajax({
			type: 'post',
			url: '<?php echo base_url()?>logged/holy_place/generateReadingPlan/',
			dataType: 'json',
			data: $('#frmSubmitPlan_'+plan_type).serialize(),
			success: function (msg) {
					if(msg.success==true)
					{
						showUIMsg('Bible reading plan has been successfully created');
						window.location.reload();
					}
					else if(msg.success==false)
					{
						showUIMsg(msg.error);
					}
			}
	});
}

function updatePlan(plan_id, is_read_updated)
{
	//$('#frmUpdatePlan').serialize()
	//alert(is_read_updated);
	showBusyScreen();
	$.ajax({
			type: 'post',
			url: '<?php echo base_url()?>logged/holy_place/update_reading_plan_by_day/',
			dataType: 'json',
			data: {'dt_to_be_updated':$('#dt_to_be_updated'+plan_id).val(), 'is_read_updated': is_read_updated},
			success: function (msg) {
				if(msg.success==true)
				{
					//alert(msg.updated_flag);
						if(msg.updated_flag == 1){
							$('#read_plan_li_'+plan_id).attr('style','background-color:#E1F2F2;');
							
							$('#update_checkbox_'+plan_id).attr('onclick','');
							$('#update_checkbox_'+plan_id).attr('onclick','updatePlan('+plan_id+','+msg.updated_flag+' )');
							$('#update_checkbox_'+plan_id).prop('checked',true);
						}
						else{
							$('#read_plan_li_'+plan_id).attr('style','background-color:#F2F2F2;');
							
							$('#update_checkbox_'+plan_id).attr('onclick','');
							$('#update_checkbox_'+plan_id).attr('onclick','updatePlan('+plan_id+','+msg.updated_flag+' )');
							$('#update_checkbox_'+plan_id).prop('checked',false);
						}
						
						$('#last_read_date_span').html(msg.last_read_date);
						$('#today_reading_assignment').html(msg.today_reading_html);
						$('#plan_progress_bar').remove();
						$('#plan_bar').html(msg.progress_bar_data);
						hideBusyScreen();
						$(".reading_plan_list").getNiceScroll().resize();
						showUIMsg(msg.data);
						//window.location.reload();
				}
				else
				{
					//$('#read_plan_li').attr('style','background-color:#E1F2F2;');
					$('#update_checkbox_'+plan_id).prop('checked',false);
					hideBusyScreen();
					showUIMsg(msg.data);
				}
			}
	});
}

