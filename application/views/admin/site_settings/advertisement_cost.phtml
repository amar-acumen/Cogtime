<script type="text/javascript">
$(document).ready(function(arg) {
	

 $('#user_alerts').submit(function(){
						
				  var datatosend=$("#user_alerts").serialize();
				 // alert(datatosend);
				  showBusyScreen();
											  
					  $.ajax({
						 type: "POST",
						 url: base_url+"admin/site_settings/advertisement_cost/edit_info",
						 data: datatosend,
						 dataType: 'json',
						 success: function(data){
							
							if(data.success==false) 
							{
								
						
								$('.error-message').each(function(i){
									$(this).attr('style','display:none');
								});
								
								
								for ( var id in data.arr_messages ){
									//alert('#err_'+id);
									if( $('#err_'+id) != null ) {
										$('#err_'+id).html(data.arr_messages[id]);
										$('#err_'+id).css('display', 'block');
									}
								}
						
								
							}
                                                        if(data.success==true) 
							{
							window.location.href = '<?= admin_base_url() ?>site-settings/advertisement_cost';
							
                                                    }
                                                    hideBusyScreen(); 
							showUIMsg(data.msg);
						 }
					   });	 
				});
 
 });
</script>